ome = () => import(/* webpackChunkNameL \"login_home_welcome\" */ '../components/Welcome')\n\n\n// import Users from '../components/user/Users'\nconst Users = () => import(/* webpackChunkNameL \"Users_Rights_Roles\" */ '../components/user/Users')\n\n// import Rights from '../components/power/Rights'\nconst Rights = () => import(/* webpackChunkNameL \"Users_Rights_Roles\" */ '../components/power/Rights')\n\n// import Roles from '../components/power/Roles'\nconst Roles = () => import(/* webpackChunkNameL \"Users_Rights_Roles\" */ '../components/power/Roles')\n\n\n// import Cate from '../components/goods/Cate'\nconst Cate = () => import(/* webpackChunkNameL \"Cate_Params\" */ '../components/goods/Cate')\n\n// import Params from '../components/goods/Params'\nconst Params = () => import(/* webpackChunkNameL \"Cate_Params\" */ '../components/goods/Params')\n\n\n// import GoodsList from '../components/goods/List'\nconst GoodsList = () => import(/* webpackChunkNameL \"GoodsList_Add\" */ '../components/goods/List')\n\n// import Add from '../components/goods/Add'\nconst Add = () => import(/* webpackChunkNameL \"GoodsList_Add\" */ '../components/goods/Add')\n\n\n\n// import Order from '../components/order/Order'\nconst Order = () => import(/* webpackChunkNameL \"Order_Report\" */ '../components/order/Order')\n\n// import Report from '../components/report/Report'\nconst Report = () => import(/* webpackChunkNameL \"Order_Report\" */ '../components/report/Report')\n\n\n\n\nVue.use(VueRouter)\n\nconst routes = [\n  { path: '/', component: Login },\n  { path: '/login', component: Login },\n  {\n    path: '/home',\n    component: Home,\n    redirect: '/welcome',\n    children: [\n      { path: '/welcome', component: Welcome },\n      { path: '/users', component: Users },\n      { path: '/rights', component: Rights },\n      { path: '/roles', component: Roles },\n      { path: '/categories', component: Cate },\n      { path: '/params', component: Params },\n      { path: '/goods', component: GoodsList },\n      { path: '/goods/add', component: Add },\n      { path: '/orders', component: Order },\n      { path: '/reports', component: Report },\n    ]\n  },\n]\n\nconst router = new VueRouter({\n  routes,\n  mode: 'history'\n})\n\n\n// 挂载路由导航守卫\nrouter.beforeEach((to, from, next) => {\n  // to 将要访问的路径\n  // from 代表从哪个路径跳转而来\n  // next 是一个函数 执行下一步 放行\n\n  if (to.path === '/login') return next()\n  // 获取token判断\n  const tokenStr = window.sessionStorage.getItem('token')\n  if (!tokenStr) return next('/login')\n  next()\n\n})\nexport default router\n"]}]}