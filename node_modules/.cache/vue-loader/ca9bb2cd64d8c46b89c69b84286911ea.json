ning\" v-else-if=\"scope.row.level === '2'\">三级</el-tag>\n          </template>\n        </el-table-column>\n      </el-table>\n    </el-card>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Rights\",\n  data() {\n    return {\n      // 所有的权限列表\n      rightsList: []\n    };\n  },\n  created() {\n    // 获取权限列表\n    this.getRightsList();\n  },\n  methods: {\n    // 获取权限列表的请求\n    async getRightsList() {\n      const { data: res } = await this.$http.get(\"rights/list\");\n      if (res.meta.status !== 200) {\n        return this.$message.error(\"获取权限列表失败！\");\n      }\n\n      this.rightsList = res.data;\n    }\n  }\n};\n</script>\n\n<style lang=\"lsee\" scoped>\n</style>"]}]}